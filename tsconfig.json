{
  "compilerOptions": {
    "target": "es2016",                                  
    "jsx": "react-jsx",                                
    "esModuleInterop": true,
    "module": "ESNext",                               
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "skipLibCheck": true,
    "declaration": true,
    "declarationDir": "types",
    "sourceMap": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "emitDeclarationOnly": true,
  }
}
